	color: #7f8083;
}
.exploremorebots .tagbutton {
	color: white;
}
#wrapperrr {
	background: #262626;
	box-shadow: 0px 0px 0px 2px#7f8083;
}
.tagbutton {
	color: white;
}
.bot-card .partnered {
	background: #231e26 !important;
}
.bot-card:hover .partnered {
	background: #242126 !important;
}
.partnered .bot-name {
	color: gold;
}
.partnered .bot-description {
	color: white !important;
}
.bot-card .partnered .bot-btns .btn:hover {
	background: #3e1f54;
	color: #e7c302 !important;
}
.bottom-bar {
	background: #677bc8 !important;
}
footer a:hover {
	color: #ceac9e;
}
.maincon .content {
	background: #222428;
	color: rgba(255, 255, 255, 0.8);
}
.content h2 {
	color: white !important;
}
.input {
	background: #34363a !important;
	border: 1px solid #2a2d32 !important;
	color: white !important;
}
.taginput {
	background: none;
	color: white;
}
.suggestedtags {
	background: #2a2d32;
}
.suggestedtags p:hover {
	background: rgba(0, 0, 0, 0.1);
}
.atag {
	background: #34363a;
}
.atag:after {
	border-left-color: #34363a;
}
.atag:before {
	background: #222428;
}
.content select {
	color: white;
	background: #34363a;
}
footer {
	background: #262626;
}
.bottom-bar {
	background: black !important;
}
.label {
	color: white !important;
}
.control .checkbox:hover {
	color: #9790a2 !important;
}
.control .checkbox strong {
	color: red !important;
}
header {
	background: #1d1f21 !important;
}
#menu {
	background: #1d1f21 !important;
}
#menu .menu ul li a:hover,
#menu .menu ul li span:hover {
	background: #35373b;
}
.content .textarea {
	background: #34363a !important;
	color: white !important;
	border: 1px solid black !important;
}
.content select {
	background: #34363a !important;
	color: white !important;
	border: 1px solid black !important;
}
.maincon .button {
	filter: invert(1) !important;
}
#bot-details-page .bot-name {
	color: gainsboro !important;
}
#createdby {
	color: grey !important;
}
.content a:hover {
	color: #334dad !important;
}
.darkthemeee h1 {
	color: white !important;
}
.darkthemeee h4 {
	color: white !important;
}
	</style>

	<body style="background-color: WHITE;">
		<br>
   		<h1 class="display-3 text-center" style="font-family: 'Dosis', cursive; text-transform: none; font-size: 70px; color: #7289DA;"><i class="fa fa-server"></i> Servidores
		</h1>

   <span class="servers btn btn-orange btn-1x"><%= bot.guilds.size; %> Servidores</span>

		<br>
		<br>
		<div id="guild-list-default">
			<div class="bot-list-section">
				<ul class="columns is-multiline is-tablet">
					<% 
bot.guilds.forEach(guild => {
  
%>
						<li class="column bot-card is-one-quarter" style="opacity: 1; transform: translateY(0px);">
							<% if(guild.id == '391269244274212866' ||  guild.id == '460314108697378826'  || guild.id == '475397487608463361') { %>
								<div class="content partnered">
									<% } else {%>
										<div class="content">
											<% } %>
												<div class="top is-flex"><span class="img-container"><div class="bot-img">
															<% if(guild.iconURL) {%>
																<img onload="" onclick="" style="cursor:pointer" alt="<%= guild.name %>" src="<%= guild.iconURL%>?size=512">
																<% } else {%>
																	<img onload="" onclick="" style="cursor:pointer" alt="<%= guild.name%>" src="https://discordapp.com/assets/dd4dbc0016779df1378e7812eabaa04d.png">
																	<% } %>
														</div></span>
													<span class="stats">
														<br>
														<span class="servers btn btn-orange btn-1x">
															<%= guild.channels.size%>
																Canais
														</span>
														<span class="servers btn btn-orange btn-1x">
															<%= guild.memberCount%>
																Membros
														</span>
													</span>
												</div>
												<div class="info">
													<% if(guild.id == '391269244274212866' || guild.id == '460314108697378826' || guild.id == '475397487608463361') { %>
														<img src="https://cdn.glitch.com/4a4f105d-84dd-4709-8bfb-16428e054a2d%2Fcertified.svg?1529543784705" alt="" title="Servidor Certificado" width="14px" height="14px"
														 style="transform: translate(0px, 6px) scale(0.8);">
														<% } %>
															<p class="bot-name" style="color: WHITE;">
																<%= guild.name %>
															</p>
															<span class="lib">
																<%= guild.owner.user.tag %>
															</span>
												</div>
												<div onclick="" class="bottom is-flex bot-btns">
													<a style="font-size: 16px;" href="/servers/<%= guild.id%>"><span class="color-medium-blue btn">Ver</span></a>
												</div>
										</div>
						</li>
						<%
})
%>
				</ul>
				</div>
			</div>
			<br>
	</body>
	<% include ./blocks/footer %>