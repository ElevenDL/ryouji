                <% } %>
                <p class="list-users-name" style="margin-left: 80px;">
                    <%= userBot1.tag%>
                </p>
                <span class="bot servers btn btn-orange btn-1x" style="margin-left: 40px; margin-top: -22px;">BOT</span>
