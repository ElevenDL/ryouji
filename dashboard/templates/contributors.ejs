    <div class="" id="BotPartners">
        <h1 class="text-center" style="margin-left: px; m">
            Network's Bot
        </h1>
        <div class="columns">
            <div class="column">
                <% let userBot1 = bot.users.get('477925836465373184');%>
                <img class="list-users-icons" src="<%= userBot1.displayAvatarURL %>" style="margin-top: 20px;">
                <% if(userBot1.presence.status == 'online') { %>
